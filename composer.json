{
    "name": "wbraganca/yii2-multiselect",
    "description": "The yii2-multiselect is a Yii 2 wrapper for [Bootstrap Multiselect](http://davidstutz.github.io/bootstrap-multiselect/).",
    "type": "yii2-extension",
    "keywords": [
        "wbraganca",
        "yii2",
        "yii2-multiselect",
        "select",
        "multiselect",
        "dropdown"
    ],
    "homepage": "https://github.com/wbraganca/yii2-multiselect",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Wanderson Bragan√ßa",
            "email": "wanderson.wbc@gmail.com",
            "homepage": "http://wbraganca.com"
        }
    ],
    "support": {
        "issues": "https://github.com/wbraganca/yii2-multiselect/issues",
        "source": "https://github.com/wbraganca/yii2-multiselect"
    },
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "bower-asset/bootstrap-multiselect": "~0.9",
        "bower-asset/multiselect": "~0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "wbraganca\\multiselect\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
